
" ----------------------------------------
"				补全插件集
"				coc 简配版	
" ----------------------------------------

" 关闭缩进及语法高亮
filetype off
"syntax off

call plug#begin('~/.vim/plugged')
	

	" coc
	" source ~/.vim/configs/plugins

call plug#end()


" ----------------------------------------
"				设置
" ----------------------------------------


" 开启文件类型
syntax on
filetype on
filetype plugin on
filetype indent on


" ----------------------------------
"			插件设置
" ----------------------------------

" source ~/.vim/configs/conf

